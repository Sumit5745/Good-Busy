    "start:changelog": "ts-node --files ./changelogs-ms/index.ts",
    "start:mail": "ts-node --files ./mail-ms/index.ts",
    "start:chat": "ts-node --files ./chat-ms/index.ts",
    "start:goal": "ts-node --files ./goal-ms/index.ts",
    "start:social": "ts-node --files ./social-ms/index.ts",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:monitor": "pm2 monit",
    "pm2:status": "pm2 status"
  },
